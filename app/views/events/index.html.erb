<h1>Events List</h1>

<ul>
	<% @events.each do |e| %>
		<li>
			<%= e.name %>
			<%= link_to 'Show', :controller => 'events', :action => 'show', :id => e %>
			<%= link_to 'Edit', :controller => 'events', :action => 'edit', :id => e %>
		</li>
	<% end %>
</ul>

<%= link_to 'New Event', :controller => 'events', :action => 'new' %>